<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
        <img src="@Guitar?.ImageThumbnailUrl" alt="">
        <div class="caption">
            <h3 class="pull-right">@Guitar?.Price.ToString("c")</h3>
            <h3>
                <a href="/pie/details/@Guitar?.Id">@Guitar?.Name</a>
            </h3>
            <p>@Guitar?.ShortDescription</p>
        </div>
    </div>
</div>